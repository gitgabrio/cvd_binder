mkdir dati_covid
cd dati_covid
wget -O - https://github.com/pcm-dpc/COVID-19/archive/master.tar.gz | tar xz --strip=2 "COVID-19-master/dati-regioni/"
cd ..
mkdir dati_vaccinazioni
cd dati_vaccinazioni
wget https://raw.github.com/italia/covid19-opendata-vaccini/master/dati/somministrazioni-vaccini-summary-latest.csv
cd ..
mkdir dati_lockdown
cd dati_lockdown
wget https://raw.github.com/gitgabrio/cvd_analysis/main/EvoluzioneLockdown.csv
cd ..
